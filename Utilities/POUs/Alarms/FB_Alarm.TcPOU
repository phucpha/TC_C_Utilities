    <Method Name="Reset" Id="{de0d8ec5-fb9f-498f-bc79-30c4e4f55aa9}">
      <Declaration><![CDATA[METHOD Reset : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT bTemporary THEN
	IF bLatched = TRUE THEN
		Reset := TRUE;
		bLatched := FALSE;
	END_IF
ELSE
	Reset := FALSE;
END_IF
]]></ST>
      </Implementation>
