END_VAR
VAR_INPUT
	bIn	:	BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[bTemporary := bIn;
IF NOT bLatched AND bIn THEN
	bLatched := TRUE;
	tsTimeOfOccur := GVL.fbMachineTime.systemTime;
END_IF
]]></ST>
      </Implementation>
    </Method>
